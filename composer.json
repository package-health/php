{
  "name": "package-health/php",
  "description": "Keep your dependencies up-to-date",
  "type": "project",
  "license": "AGPL-3.0-or-later",
  "authors": [
    {
      "name": "Flavio Heleno",
      "email": "flaviohbatista@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "PackageHealth\\PHP\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PackageHealth\\PHP\\Test\\": "tests"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "preferred-install": "dist",
    "allow-plugins": {
      "infection/extension-installer": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^8.1",
    "ext-filter": "*",
    "ext-json": "*",
    "ext-pcntl": "*",
    "ext-pdo": "*",
    "ext-posix": "*",
    "ext-redis": "*",
    "ext-zlib": "*",
    "badges/poser": "^2.3",
    "composer/metadata-minifier": "^1.0",
    "composer/semver": "^3.3",
    "courier/courier": "dev-main",
    "courier/locator-container": "dev-main",
    "courier/middleware-envelope-compression": "dev-main",
    "courier/serializer-igbinary": "dev-main",
    "courier/transport-amqp": "dev-main",
    "flavioheleno/kolekto": "dev-main",
    "kriswallsmith/buzz": "^1.2",
    "middlewares/minifier": "^2.0",
    "middlewares/trailing-slash": "^2.0",
    "monolog/monolog": "^3.2",
    "nyholm/dsn": "^2.0",
    "nyholm/psr7": "^1.5",
    "nyholm/psr7-server": "^1.0",
    "php-di/php-di": "^7.0",
    "phpunit/php-timer": "^5.0",
    "psr/cache": "^1.0",
    "psr/container": "^2.0",
    "psr/http-message": "^1.0",
    "psr/log": "^2.0",
    "robmorgan/phinx": "^0.13",
    "slim/http-cache": "^1.1",
    "slim/slim": "^4.11",
    "slim/twig-view": "^3.3",
    "symfony/console": "^6.2",
    "tedivm/stash": "^0.17",
    "vlucas/phpdotenv": "^5.5"
  },
  "require-dev": {
    "infection/infection": "^0.26",
    "php-parallel-lint/php-parallel-lint": "^1.3",
    "phpstan/phpstan": "^1.9",
    "phpunit/phpunit": "^9.5",
    "psy/psysh": "^0.11",
    "roave/security-advisories": "dev-latest",
    "squizlabs/php_codesniffer": "^3.7",
    "vimeo/psalm": "^5.4"
  }
}
